{
	"name": "completionism-react",
	"version": "1.0.0",
	"description": "Track collections in World of Warcraft",
	"main": "webpack.config.js",
	"scripts": {
		"start": "node src/server/start.js",
		"dev": "npm run sass & webpack-dev-server & npm run server",
		"build": "webpack",
		"sass": "./node_modules/.bin/node-sass ./public/sass/main.scss ./public/style.css & npm run sass:watch",
		"sass:watch": "./node_modules/.bin/node-sass --watch ./public/sass/main.scss ./public/style.css",
		"server": "nodemon --ignore ./src/client/ ./src/server/start.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JWLD/completionism-react.git"
	},
	"author": "Jack Davies",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/JWLD/completionism-react/issues"
	},
	"homepage": "https://github.com/JWLD/completionism-react#readme",
	"dependencies": {
		"axios": "^0.17.1",
		"dotenv": "^4.0.0",
		"express": "^4.16.2",
		"lodash": "^4.17.4",
		"mysql": "^2.15.0",
		"node-sass": "^4.7.2",
		"prop-types": "^15.6.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-icons": "^2.2.7",
		"react-redux": "^5.0.6",
		"react-router-dom": "^4.2.2",
		"redux": "^3.7.2",
		"redux-form": "^7.2.1",
		"redux-thunk": "^2.2.0",
		"reselect": "^3.0.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.1.2",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.7",
		"eslint": "^4.15.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.5.1",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.1",
		"webpack": "^3.10.0",
		"webpack-dev-server": "^2.9.7"
	}
}
